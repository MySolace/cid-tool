{
  "name": "futoin-cid",
  "version": "0.7.99",
  "plugins": {
    "release": "futoin.cid.misc.releasetool"
  },
  "vcs": "git",
  "rms": "twine",
  "rmsRepo": "ignored",
  "tools": {
    "release": "embedded",
    "python": "*",
    "setuptools": true
  },
  "entryPoints": {
    "main": {
      "tool": "exe",
      "path": "bin/cid"
    }
  },
  "actions": {
    "autopep8": "autopep8 -i -r futoin",
    "check": [
      "pylint -E futoin --ignore=contrib",
      "@default"
    ]
  }
}
